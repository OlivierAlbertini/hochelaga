<nav aria-label="Page navigation example">
  <ul class="pagination {{ paginationClass }}">
    <li class="page-item">
      <a class="page-link" href="#" aria-label="Previous">
        <span class="vdmi ic-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
    </li>
  {{# paginationList }}
    <li class="page-item {{ class }}"><a class="page-link" href="{{ url}}">{{ text }}</a></li>
  {{/ paginationList }}
    <li class="page-item">
      <a class="page-link" href="#" aria-label="Next">
        <span class="vdmi ic-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </li>
  </ul>
</nav>