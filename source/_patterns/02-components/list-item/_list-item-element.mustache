    {{/ extraContentBold }}
    {{{extraContent}}}
  </div>
  {{/ listExtraContent}}
  {{/ listItemAction}}
  {{^ listItemAction}}
  <div class="list-item-content">
    {{# listItemLabel }}
    <div class="list-item-label">{{listItemLabel}}</div>
    {{/ listItemLabel }}
    {{# listExtraContent}}
    <div class="list-item-content">
      {{# extraContentAction}}
      <div class="list-item-action{{# classes}} {{classes}}{{/classes}}">
        <a href="{{url}}"{{# actionClasses}} class="{{actionClasses}}"{{/ actionClasses}}>
          <span class="link-label">{{ text }}</span>
        </a>
      </div>
      {{/ extraContentAction}}
      {{# extraContentBold }}
      <div class="font-weight-bold">{{{extraContentBold}}}</div>
      {{/ extraContentBold }}
