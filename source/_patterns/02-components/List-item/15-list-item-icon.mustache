<div class="list list-icon">
	{{# list }}
  <div class="list-item-icon">
    {{# listItemAction}}
    <a class="list-item-icon-action" href="{{ url }}">
      <div class="flex-shrink-0">
        <svg class="icon" viewBox="0 0 24 24" role="icon"><path d="M14.988 18.090c0.406-0.375 1.039-0.349 1.413 0.057s0.349 1.039-0.057 1.413c-0.797 0.736-2.030 1.803-3.701 3.206-0.35 0.294-0.855 0.313-1.227 0.046-5.57-4.004-8.416-8.273-8.416-12.812 0-4.785 4.117-9 9-9s9 4.215 9 9c0 2.099-0.613 4.151-1.826 6.143-0.287 0.472-0.902 0.621-1.374 0.334s-0.621-0.902-0.334-1.374c1.027-1.687 1.534-3.383 1.534-5.103 0-3.69-3.232-7-7-7s-7 3.31-7 7c0 3.636 2.283 7.213 6.958 10.728 1.346-1.137 2.357-2.017 3.030-2.638zM12 14c-2.209 0-4-1.791-4-4s1.791-4 4-4c2.209 0 4 1.791 4 4s-1.791 4-4 4zM12 12c1.105 0 2-0.895 2-2s-0.895-2-2-2c-1.105 0-2 0.895-2 2s0.895 2 2 2z"></path></svg>
      </div>
      <div class="list-item-icon-label">{{text}}</div>
    </a>
    {{# listExtraContent}}
    <div class="list-item-icon-content ml-5">{{{listExtraContent}}}</div>
    {{/ listExtraContent}}
    {{/ listItemAction}}
    {{^ listItemAction}}
      <div class="flex-shrink-0">
        <svg class="icon" viewBox="0 0 24 24" role="icon"><path d="M14.988 18.090c0.406-0.375 1.039-0.349 1.413 0.057s0.349 1.039-0.057 1.413c-0.797 0.736-2.030 1.803-3.701 3.206-0.35 0.294-0.855 0.313-1.227 0.046-5.57-4.004-8.416-8.273-8.416-12.812 0-4.785 4.117-9 9-9s9 4.215 9 9c0 2.099-0.613 4.151-1.826 6.143-0.287 0.472-0.902 0.621-1.374 0.334s-0.621-0.902-0.334-1.374c1.027-1.687 1.534-3.383 1.534-5.103 0-3.69-3.232-7-7-7s-7 3.31-7 7c0 3.636 2.283 7.213 6.958 10.728 1.346-1.137 2.357-2.017 3.030-2.638zM12 14c-2.209 0-4-1.791-4-4s1.791-4 4-4c2.209 0 4 1.791 4 4s-1.791 4-4 4zM12 12c1.105 0 2-0.895 2-2s-0.895-2-2-2c-1.105 0-2 0.895-2 2s0.895 2 2 2z"></path></svg>
      </div>
      <div class="list-item-icon-content">
        {{# text }}
        <div class="list-item-icon-label">{{text}}</div>
        {{/ text }}
        {{# listExtraContent}}
        <div>{{{listExtraContent}}}</div>
        {{/ listExtraContent}}
      </div>
    {{/ listItemAction}}
  </div>
  {{/ list }}
</div>
