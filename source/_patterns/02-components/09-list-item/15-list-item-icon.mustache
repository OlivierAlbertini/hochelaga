<div class="list list-icon">
	{{# list }}
  <div class="list-item-icon">
    {{# listItemAction}}
    <div class="list-item-icon-action">
      <a href="{{url}}" class="link-has-icon">
        <div><span class="icon {{icon-name}}"></span></div>
        <div><span class="link-icon-label">{{ text }}</span></div>
      </a>
    </div>
    {{# listExtraContent}}
    <div class="list-item-icon-content ml-5">{{{listExtraContent}}}</div>
    {{/ listExtraContent}}
    {{/ listItemAction}}
    {{^ listItemAction}}
    <span class="icon {{icon-name}}" aria-hidden="true"></span>
    <div class="list-item-icon-content">
      {{# text }}
      <div class="list-item-icon-label">{{text}}</div>
      {{/ text }}
      {{# listExtraContent}}
      <div>{{{listExtraContent}}}</div>
      {{/ listExtraContent}}
    </div>
    {{/ listItemAction}}
  </div>
  {{/ list }}
</div>
