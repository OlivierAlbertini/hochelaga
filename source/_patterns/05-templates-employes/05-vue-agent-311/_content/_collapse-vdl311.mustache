{{# gdc}}
<div class="mb-3">
  <h2 class="font-size-base-interface mb-1">{{ heading }}</h2>
  <ul class="list {{listClass}}">
  {{# list }}
    <li class="list-item-icon">
      <div class="mr-2">
        <img src="{{ src}}" alt="{{alt}}" />
      </div>
      <div class="list-item-icon-content">
        {{# text }}
        <div class="list-item-icon-label">{{text}}</div>
        {{/ text }}
        {{# listExtraContent}}
        <div {{#listExtraContentClass}}class="{{listExtraContentClass}}"{{/listExtraContentClass}}>{{{listExtraContent}}}</div>
        {{/ listExtraContent}}
      </div>
    </li>
    {{/ list }}
  </ul>
</div>
{{/ gdc}}

{{# question-type}}
<div class="questions-container element-mb-sm">
  {{# question-item}}
  <div class="question-item">
    <h2 class="font-size-base-interface mb-1">{{ question }}</h2>
    <div class="wrapper wrapper-answer mb-3 rm-last-child-mb">
      {{{ answer}}}
    </div>
  </div>
  {{/ question-item}}
</div>
{{/ question-type}}

