<div class="vdm-select-wrapper {{ selectWrapperClasses }}">
    <!-- "selectHasLabel" : true inside local data file to show select label  -->
    {{# selectHasLabel }}
    <label for="{{ formAttributes.selectID }}">Label</label>
    {{/ selectHasLabel }}
    <select class="form-control vdm-select {{ selectWrapperClasses }}" name="{{ formAttributes.selectID }}" id="{{ formAttributes.selectID }}"> 
        <option value="">Filter by</option> 
        {{# listItems.five }}
            {{> atoms-option }}
        {{/ listItems.five }}
    </select>
</div>

