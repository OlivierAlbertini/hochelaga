<h2 class="pl-stroked-header">Checkboxes</h2>
<form>
    <div class="checkbox vdm-checkbox">
        <label>
            <input type="checkbox">Checkbox
        </label>
    </div>
    <div class="checkbox vdm-checkbox ">
        <label>
            <input type="checkbox" checked="checked">Checked checkbox
        </label>
    </div>
    <div class="checkbox vdm-checkbox" disabled>
        <label>
            <input type="checkbox">Disabled checkbox
        </label>
    </div>
    <div class="checkbox vdm-checkbox" checked="checked" disabled>
        <label>
            <input type="checkbox">Disabled and checked checkbox
        </label>
    </div>
</form>

<h4 class="pl-stroked-header">Inline</h4>
<form>
    <label class="checkbox vdm-checkbox checkbox-inline">
        <input type="checkbox">1
    </label>
    <label class="checkbox vdm-checkbox checkbox-inline">
        <input type="checkbox" checked="checked">2
    </label>
</form>

<h4 class="pl-stroked-header">Inline</h4>
<form>
    <label class="checkbox vdm-checkbox checkbox-inline">
        <input type="checkbox">1
    </label>
    <label class="checkbox vdm-checkbox checkbox-inline">
        <input type="checkbox" checked="checked">2
    </label>
</form>

