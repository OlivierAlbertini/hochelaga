{{> styles-link-skip-to-main-content }}
{{> patterns-header-portal }}

<main class="main-content {{ pageClasses }}" id="main-content" aria-label="Main Content">

  {{> patterns-content-header }}

  <!-- Teaser -->
  {{# has-lead-text }}
  {{> components-lead }}
  {{/ has-lead-text }}

  <!-- Begin main-content region-->
  <div id="navTop" class="page-section {{ pageSectionClasses }}">
    {{# servicesSection}}
    <div class="container">
      <div class="row cards {{ cards.classes }}">
        {{# cards.card-item}}
        <div class="{{ cards.bsgrid-classes }}">
          {{> components-card-item }}
        </div>
        {{/ cards.card-item}}
      </div>
    </div>
    {{/ servicesSection}}
  </div>

  {{# hasFeedbackForm}}
  {{> patterns-feedback-form}}
  {{/ hasFeedbackForm }}
</main>

{{> patterns-footer-portal }}

