          <div class="content-module-stacked">
            <h2>Liste automatisée des programmes</h2>
            {{> components-list-item-link}}
          </div>
          {{/ listAutoProgrammes}}

          {{# listAutoArticles}}
          <div class="content-module-stacked">
            <h2>Liste automatisée des conseils-articles</h2>
            {{> components-list-item-link}}
          </div>
