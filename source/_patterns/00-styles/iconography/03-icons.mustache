<div class="cotainer-fluid pl-atom-icons-grid">
	
	<h4 class="pl-pattern-subtitle">Editorial</h4>
  	<ul class="row list-unstyled">
  		{{#icon-editorial }}
	   		<li class="col-6 col-sm-4 col-md-4 col-lg-3">
		      	<div class="pl-atom-icon-item icon-editorial">
		      		<svg class="icon {{ icon-name }}" viewBox="0 0 48 48"> 
    					<use xlink:href="../../images/icons/ic-editorial-defs.svg#{{ icon-name }}" x="0" y="0" />
					</svg>
		       		<div class="ic-name">{{ icon-name }}</div>
		      	</div>
		    </li>
		{{/icon-editorial}}
  	</ul>

  	<h4 class="pl-pattern-subtitle">Utility</h4>
  	<ul class="row list-unstyled">
  		{{#icon-utility }}
	   		<li class="col-6 col-sm-4 col-md-4 col-lg-3">
		      	<div class="pl-atom-icon-item icon-utility">
		      		<svg class="icon {{ icon-name }}" viewBox="0 0 24 24"> 
		      			<path d="{{ icon-path }}"></path>
					</svg>
		       		<span class="ic-name">{{ icon-name }}</span>
		      	</div>
		    </li>
		{{/icon-utility}}
  	</ul>

</div>