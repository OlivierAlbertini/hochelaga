{{> atoms-link-skip-to-main-content }}
{{> organisms-header-portal }}

<main class="main-content {{ pageClasses }}" id="main-content" aria-label="Main Content">

  {{> organisms-content-header }}

  <!-- Teaser -->
  {{# has-lead-text }}
  {{> molecules-lead }}
  {{/ has-lead-text }}

  {{# featuredSection}}
    {{> molecules-card }}
  {{/ featuredSection}}

  {{# demandesListes }}
  <section class="page-section {{cards.section-classes}}">
    <div class="container">
      <div class="row">
        {{# cards.headings }}
        <div class="col-12">
          <h2 class="{{ cards.headings.class }}">{{ cards.headings.text }}</h2>
        </div>
        {{/ cards.headings }}

        <div class="col-12">
          {{> molecules-search-field+dropdown }}
        </div>

      </div>
      <div class="row cards {{ cards.classes }}">
        {{# cards.card-item }}
        <div class="{{ cards.bsgrid-classes }}">
          {{> molecules-card-item }}
        </div>
        {{/ cards.card-item }}
      </div>
      <div class="row">
        <div class="col-12">
          {{# hasPagination }}
            {{> molecules-pagination("paginationClass": justify-content-center) }}
          {{/ hasPagination }}
        </div>
      </div>
    </div>
  </section>
  {{/ demandesListes }}
</main>

{{> organisms-footer-portal }}
