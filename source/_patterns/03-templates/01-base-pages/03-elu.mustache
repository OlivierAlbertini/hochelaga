{{> atoms-link-skip-to-main-content }}
{{> organisms-header-portal }}

<main class="main-content {{ pageClasses }}" id="main-content" aria-label="Main Content">

  {{> organisms-content-header-profile }}

  <!-- Begin main-content region-->
  <div class="region-content {{ regionMainClasses }}">
    <div class="container {{ regionMainContainerClasses }}">
      <div class="row">
        <aside class="col-12 col-lg-4 sidebar-left" role="complementary">
          {{> organisms-sidebar-block-coordinates }}
        </aside>
        <article class="col-12 {{ regionMainContentClasses }}">
          <div class="page-section pt-0">
            <h2 class="section-heading">Rôles et responsabilités</h2>
            {{# cards-roles }}
            <div class="cards">
              {{# cards.card-item }}
                {{> molecules-card-item}}
              {{/ cards.card-item }}
            </div>
            {{/ cards-roles }}
          </div>
          <div class="page-section pt-0">
            <h2 class="section-heading">Autres responsabilités</h2>
            {{> molecules-list-group }}
          </div>

        </article>
      </div>
    </div>
  </div>

  {{# hasFeedbackForm}}
  {{> organisms-feedback-form}}
  {{/ hasFeedbackForm }}
</main>

{{> organisms-footer-portal }}
