{{# gdc}}
<div class="mb-1">
  {{# heading}}<h2 class="font-size-base-interface mb-1">{{ heading }}</h2>{{/ heading}}
  <ul class="list{{# listClass }} {{ listClass }}{{/ listClass }}">
    {{# list }}
    <li class="list-item-icon">
      <div class="mr-2">
        <img src="{{ src}}" alt="{{alt}}" width="24" height="24"/>
      </div>
      <div class="list-item-icon-content">
        {{# listItemLabel }}
        <div class="list-item-icon-label">{{listItemLabel}}</div>
        {{/ listItemLabel}}
        {{# listItemContent}}
        {{#listExtraContentClass}}
        <div class="{{listExtraContentClass}}">
        {{/listExtraContentClass}}
          {{# extraContent}}
          <div{{#class}} class="{{class}}"{{/class}}>{{{content}}}</div>
          {{/ extraContent}}
        {{#listExtraContentClass}}
        </div>
        {{/ listExtraContentClass}}
        {{/ listItemContent}}
      </div>
    </li>
    {{/ list }}
  </ul>
</div>
{{/ gdc}}

